{
  "name": "Design Column Overlay",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": { "16": "grid16.png", "48": "grid48.png", "128": "grid128.png" },
  "permissions": ["tabs", "activeTab", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
