{
  "name": "Design Column Overlay",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "column16.png",
    "48": "column48.png",
    "128": "column128.png"
  },
  "permissions": ["tabs", "activeTab", "http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
